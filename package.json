{
  "name": "qucikky-redis-client",
  "version": "0.1.1-0",
  "author": "vitoaz(vitoaaazzz@gmail.com)",
  "description": "QuickkyRedisClient",
  "license": "MIT",
  "gypfile": false,
  "scripts": {
    "build-win32": "node .electron-vue/build.js && electron-builder --win --ia32",
    "build-linux": "node .electron-vue/build.js && electron-builder --linux",
    "build-mac": "node .electron-vue/build.js && electron-builder --mac",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js"
  },
  "build": {
    "productName": "QuickkyRedisClient",
    "appId": "com.hivito.quickky-redis-client",
    "directories": {
      "output": "dist"
    },
    "nsis": {
      "guid": "qucikky-redis-client",
      "include": ".electron-vue/install.nsh",
      "shortcutName": "QuickkyRedisClient",
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "static/icons/fav.ico",
      "uninstallerIcon": "static/icons/fav.ico",
      "installerHeaderIcon": "static/icons/fav.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "menuCategory": false
    },
    "files": [
      "dist/electron/**/*"
    ],
    "extraResources": [
      {
        "from": "resources",
        "to": "../resources"
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "static/icons/icon.icns"
    },
    "win": {
      "icon": "static/icons/fav.ico"
    },
    "linux": {
      "icon": "static/icons"
    }
  },
  "dependencies": {
    "axios": "0.26.1",
    "clipboard": "2.0.10",
    "core-js": "3.22.0",
    "crypto-js": "4.1.1",
    "electron-log": "4.3.5",
    "element-ui": "2.15.8",
    "ioredis": "4.28.5",
    "normalize.css": "7.0.0",
    "sortablejs": "1.15.0",
    "sprintf-js": "1.1.2",
    "uuid": "8.3.2",
    "vue": "2.6.14",
    "vue-electron": "1.0.6",
    "vue-i18n": "8.27.1",
    "vue-router": "3.5.3",
    "vuex": "3.6.2",
    "vuex-electron": "1.0.3"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/eslint-parser": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/runtime": "7.17.9",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "babel-loader": "8.2.4",
    "babel-minify-webpack-plugin": "0.3.1",
    "cfonts": "2.1.2",
    "chalk": "2.4.1",
    "copy-webpack-plugin": "4.5.1",
    "cross-env": "5.1.6",
    "css-loader": "0.28.11",
    "del": "3.0.0",
    "devtron": "1.4.0",
    "electron": "2.0.18",
    "electron-builder": "21.2.0",
    "electron-debug": "3.2.0",
    "electron-devtools-installer": "3.2.0",
    "electron-rebuild": "2.3.5",
    "eslint": "7.32.0",
    "eslint-plugin-vue": "6.2.2",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "inject-loader": "4.0.1",
    "lodash": "4.17.21",
    "mini-css-extract-plugin": "1.6.2",
    "multispinner": "0.2.1",
    "node-loader": "0.6.0",
    "node-sass": "4.14.1",
    "require-dir": "1.0.0",
    "sass-loader": "7.0.3",
    "spectron": "3.8.0",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "vue-html-loader": "1.2.4",
    "vue-loader": "15.2.4",
    "vue-style-loader": "4.1.0",
    "vue-template-compiler": "2.6.14",
    "webpack": "4.15.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4",
    "webpack-hot-middleware": "2.22.2",
    "webpack-merge": "4.1.3"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  }
}
